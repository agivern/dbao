<?php
namespace DBAO\BDDBundle\Entity;
use Doctrine\ORM\Mapping as ORM;
use Symfony\Component\Validator\Constraints as Assert;

/**
 * @ORM\Entity
 */
class Slide
{
    /**
     * @ORM\GeneratedValue
     * @ORM\Id
     * @ORM\Column(type="integer")
     */
    private $id;

    /**
     * @ORM\ManyToOne(targetEntity="DBAO\BDDBundle\Entity\Realisation")
     * @ORM\JoinColumn(nullable=false)
     */
    private $realisation;

    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set realisation
     *
     * @param \DBAO\BDDBundle\Entity\Realisation $realisation
     * @return Slide
     */
    public function setRealisation(\DBAO\BDDBundle\Entity\Realisation $realisation)
    {
        $this->realisation = $realisation;
    
        return $this;
    }

    /**
     * Get realisation
     *
     * @return \DBAO\BDDBundle\Entity\Realisation 
     */
    public function getRealisation()
    {
        return $this->realisation;
    }
}